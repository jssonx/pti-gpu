{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug unitrace with gdb-oneapi",
        "type": "cppdbg",
        "request": "launch",
        "program": "/storage/users/yuning/pg/ra/pti-gpu/tools/unitrace/build/unitrace",
        "args": [
          "--stall-sampling",
          "/home/users/yuning/pg/qahpct/minitest/intelgpu/single.sycloffload.ipcx/single.sycloffload.icpx.intelgpu"
        ],
        "environment": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/home/users/yuning/intel/oneapi/debugger/latest/bin/gdb-oneapi",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          },
          {
            "description": "Set follow fork mode to debug child processes",
            "text": "set follow-fork-mode child",
            "ignoreFailures": true
          }
        ]
      },
      {
        "name": "Attach to Process",
        "type": "cppdbg",
        "request": "attach",
        "program": "/home/users/yuning/pg/ra/pti-gpu/tools/unitrace/build/unitrace",
        "processId": "${command:pickProcess}",
        "MIMode": "gdb",
        "miDebuggerPath": "/home/users/yuning/intel/oneapi/debugger/latest/bin/gdb-oneapi",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }